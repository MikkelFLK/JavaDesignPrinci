<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="289.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javadesignprinci.GUI.Controller.CreateUserViewController">
   <children>
      <Label layoutX="90.0" layoutY="14.0" text="Create User">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <JFXTextField fx:id="createUserUsernameField" layoutX="8.0" layoutY="76.0" promptText="Username" />
      <JFXTextField fx:id="CreateUserEmailField" layoutX="189.0" layoutY="76.0" promptText="Email" />
      <JFXTextField fx:id="createUserPasswordField" layoutX="8.0" layoutY="155.0" promptText="Password" />
      <JFXTextField fx:id="createUserPasswordAgainField" layoutX="189.0" layoutY="155.0" promptText="Password Again" />
      <JFXButton fx:id="createUserCancelButton" layoutX="190.0" layoutY="236.0" onAction="#createUserCancelButtonOnAction" style="-fx-background-color: lightgrey;" text="Cancel">
         <font>
            <Font size="18.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="createUserCreateButton" layoutX="68.0" layoutY="236.0" onAction="#createUserCreateButtonOnAction" style="-fx-background-color: lightgrey;" text="Create">
         <font>
            <Font size="18.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
